// $TEST$ apply r"^Set Argument.*"

package tests.codeActions.quickfixes.setArgumentsToParameterBounds

// single argument, single bound, int
segment singleBoundIntG(const x: Int)       where {x > 0}{}
segment singleBoundIntL(const x: Int)       where {x < 0}{}
segment singleBoundIntGE(const x: Int)      where {x >= 0}{}
segment singleBoundIntLE(const x: Int)      where {x <= 0}{}

// single argument, single bound, float
segment singleBoundFloatG(const x: Float)   where {x > 0.0}{}
segment singleBoundFloatL(const x: Float)   where {x < 0.0}{}
segment singleBoundFloatGE(const x: Float)  where {x >= 0.0}{}
segment singleBoundFloatLE(const x: Float)  where {x <= 0.0}{}

// single argument, multiple bounds, int
segment multiBoundsInt(const x: Int)        where {x > 0, x < 10}{}
segment multiBoundsInt2(const x: Int)       where {x >= 10, x >= 0}{}

// single argument, multiple bounds, float
segment multiBoundsFloat(const x: Float)    where {x > 0.0, x < 10.0}{}
segment multiBoundsFloat2(const x: Float)   where {x >= 10.0, x >= 0.0}{}


